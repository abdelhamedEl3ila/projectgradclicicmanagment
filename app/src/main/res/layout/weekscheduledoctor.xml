<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/background"
    tools:context=".doctor.ScheduldocFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="@dimen/_300sdp"
        android:layout_height="200dp"
        android:elevation="@dimen/_2sdp"
        android:background="@drawable/customedittext"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.107">

        <LinearLayout
            android:id="@+id/sunlin"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.072"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="S"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarsun"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progress="50"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/monlin"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toEndOf="@+id/sunlin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:id="@+id/monday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="M"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarmon"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/thulin"

            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toEndOf="@+id/monlin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:id="@+id/tuesday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="T"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarrthu"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/wenlin"

            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toEndOf="@+id/thulin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="W"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarwen"

                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/therlin"

            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toEndOf="@+id/wenlin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="Th"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarther"

                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/frilin"

            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toEndOf="@+id/therlin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="F"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarfri"

                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/satlin"

            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toEndOf="@+id/frilin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/g_bold"
                android:text="S"
                android:textColor="@color/secondcolor"
                android:textSize="20sp"
                android:textStyle="bold"

                />

            <ProgressBar
                android:id="@+id/progressBarsat"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="5dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:progressDrawable="@drawable/scheduleprogressbar" />
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>