<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background"
    tools:context=".doctor.DoctorActivity">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/framlayoutdoc"
        />
    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bottomnavdoc"
        app:mbn_circleColor="@color/white"
        app:mbn_selectedIconColor="@color/secondcolor"
        app:mbn_backgroundBottomColor="@color/white"
        app:mbn_countTextColor="@color/white"
        app:mbn_defaultIconColor="@color/gray"
        />
</LinearLayout>